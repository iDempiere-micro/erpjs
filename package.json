{
  "name": "erp3",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "PRIVATE",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "format:check": "prettier --check  \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "organize": "organize-imports-cli tsconfig.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/erpjs/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --resolve-plugins-relative-to .",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --resolve-plugins-relative-to .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/erpjs/test/jest-e2e.json",
    "pg": "docker run --rm --name pg-docker -e POSTGRES_PASSWORD=Coggel86 -e POSTGRES_DB=gt2 -d -p 5432:5432 postgres:12",
    "generate-db": "npx typeorm-model-generator -h localhost -d gt2 -u postgres -x Coggel86 -e postgres -o ./apps/api/src/model/generated -s public",
    "keycloak": "docker run -p 8080:8080 -e KEYCLOAK_USER=admin -e KEYCLOAK_PASSWORD=admin quay.io/keycloak/keycloak:11.0.2",
    "typeorm-migration": "./node_modules/.bin/typeorm migration:create -d apps/api/src/app/migrations -n",
    "typeorm-migration-generate": "./node_modules/.bin/typeorm migration:generate -d apps/api/src/app/migrations -n",
    "docker:build:api": "docker build -t erpjs3-api . -f api.dockerfile",
    "docker:run:api": "docker run --expose 3333 -env-file=.env -e POSTGRES_HOST=host.docker.internal -p 3333:3333 erpjs3-api"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/typeorm": "^10.0.1",
    "@types/multer": "^1.4.11",
    "async-lock": "^1.4.1",
    "aws-sdk": "^2.1515.0",
    "axios": "^1.6.2",
    "azure-storage": "^2.10.7",
    "deep-equal": "^2.2.3",
    "graphql": "^16.8.1",
    "moment": "^2.29.4",
    "organize-imports-cli": "^0.10.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-loader": "^9.4.3",
    "typeorm": "^0.3.17",
    "xml-serializer-ts": "^1.0.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/async-lock": "^1.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
